<div class="row">
	<div class="column large-8">
		<div class="photo">
			<img src="img/helix-nebula-1400x1400.jpg" >
			<div class="photo-credit-wrapper">
				<div class="photo-credit-name">{{ data[all.top].cover.author }} /
					<div class="photo-credit-title">daily bruin</div>
				</div>
			</div>
		</div>

	</div>
	<div class="column large-4">
		<div class="main-story">
			<div class="category">
				{{ data[all.top].category }}
			</div>
			<div class="title">
				{{ data[all.top].title }}
			</div>
			<div class="author">
				BY {{ data[all.top].author }}
			</div>
			<div class="description">
        {{ data[all.top].excerpt }}
      </div>
		</div>

  </div>
  <hr class="main-story__black-line">
  <hr class="main-story__black-line">
  <div class="main-story__space"></div>
</div>
